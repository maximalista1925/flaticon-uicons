{
  "files.associations": {
    "**/src/main/resources/deploy.json": "json",
    "*.ttml": "xml",
    "*.ttss": "css"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".mule": true
  },
